CREATE TABLE MARKET_BILL_TB (
    BILLID VARCHAR2(12 BYTE) PRIMARY KEY,
    CUSTOMERNAME VARCHAR2(50 BYTE) NOT NULL,
    ITEMNAME VARCHAR2(50 BYTE) NOT NULL,
    PURCHASE_DATE DATE NOT NULL,
    QUANTITY NUMBER,
    PRICE NUMBER,
    TOTALAMOUNT NUMBER,
    REMARKS VARCHAR2(100 BYTE)
);

CREATE SEQUENCE MARKET_BILL_SEQ
START WITH 10
INCREMENT BY 1
MAXVALUE 99;
